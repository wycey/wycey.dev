{
  "name": "@wycey/wycey-monorepo",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "please-use-bun",
    "npm": "please-use-bun",
    "yarn": "please-use-bun",
    "pnpm": "please-use-bun",
    "bun": ">=1"
  },
  "private": true,
  "homepage": "https://wycey.dev",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "preinstall": "bunx only-allow bun",
    "prepare": "run-s setup:*",
    "setup:git": "lefthook install",
    "build": "dotenv -- turbo run build",
    "deploy": "turbo run deploy",
    "dev": "turbo run dev",
    "dev.debug": "turbo run dev.debug",
    "fmt": "biome format --write --changed .",
    "fmt.check": "biome format --changed .",
    "lint": "turbo run lint",
    "lint.ci": "turbo run lint.ci",
    "lint.fix": "turbo run lint.fix",
    "preview": "turbo run preview",
    "start": "turbo run start",
    "turbo": "turbo"
  },
  "dependencies": {
    "@biomejs/biome": "^1.6.1",
    "@cloudflare/workers-types": "^4.20231025.0",
    "@types/node": "^20.8.10",
    "dotenv-cli": "^7.3.0",
    "lefthook": "^1.6.5",
    "npm-run-all": "^4.1.5",
    "tslib": "^2.6.2",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
