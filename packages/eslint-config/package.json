        "eslint-plugin-import": "npm:eslint-plugin-i@2.26.0-2",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-regexp": "^1.12.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "eslint-plugin-unused-imports": "^2.0.0"
