{
  "name": "@wycey/web",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Mido",
    "email": "mido@wycey.dev",
    "url": "https://mido.wycey.dev"
  },
  "homepage": "https://wycey.dev",
  "type": "module",
  "main": "./server/entry.ssr.js",
  "scripts": {
    "build": "dotenv -- qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ .",
    "lint.ci": "eslint -f @microsoft/eslint-formatter-sarif -o ../../web.sarif --cache --cache-location node_modules/.cache/eslint/ .",
    "lint.fix": "eslint --fix --cache --cache-location node_modules/.cache/eslint/ .",
    "preview": "vite preview --open",
    "preview.build": "qwik build preview",
    "serve": "wrangler pages dev ./dist",
    "qwik": "qwik"
  },
  "dependencies": {
    "@radix-ui/colors": "^3.0.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.17",
    "@builder.io/qwik-city": "^1.2.17",
    "@vanilla-extract/css": "^1.13.0",
    "@wycey/tsconfig": "workspace:*",
    "eslint-config-wycey": "workspace:*",
    "eslint-plugin-qwik": "^1.2.17",
    "globals": "^13.23.0",
    "node-fetch": "^3.3.2",
    "sass": "^1.69.5",
    "styled-vanilla-extract": "^0.5.12",
    "wrangler": "^3.0.0"
  }
}
