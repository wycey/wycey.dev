{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "dotEnv": [
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ],
      "outputs": ["dist/**", "server/**"]
    },
    "preview.build": {
      "dependsOn": ["^preview.build"],
      "dotEnv": [
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ],
      "outputs": ["dist/**", "server/**"]
    },
    "preview": {
      "dependsOn": ["preview.build"],
      "cache": false
    },
    "deploy": {
      "dependsOn": ["build"],
      "inputs": ["dist/**", "functions/**"]
    },
    "dev": {
      "dotEnv": [
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ],
      "cache": false
    },
    "dev.debug": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "lint.ci": {
      "outputs": []
    },
    "lint.fix": {
      "outputs": []
    }
  }
}
